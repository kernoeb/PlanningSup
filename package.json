{
  "name": "planningsup",
  "version": "2.14.12",
  "private": true,
  "scripts": {
    "build": "node --max-old-space-size=2048 node_modules/nuxt/bin/nuxt.js build",
    "dev": "docker-compose -f docker-compose-dev.yml up -d --remove-orphans && NO_BREE=true node --inspect --max-old-space-size=2048 node_modules/nuxt/bin/nuxt.js",
    "dev-server": "docker-compose -f docker-compose-dev.yml up -d --remove-orphans && NO_BREE=true nodemon --inspect --max-old-space-size=2048 server/",
    "dev-server:bree": "docker-compose -f docker-compose-dev.yml up -d --remove-orphans && nodemon --inspect --max-old-space-size=2048 server/",
    "dev-server:bree:darwin-arm64": "docker-compose -f docker-compose-dev-arm64.yml up -d --remove-orphans && nodemon --inspect --max-old-space-size=2048 server/",
    "dev-server:darwin-arm64": "docker-compose -f docker-compose-dev-arm64.yml up -d --remove-orphans && NO_BREE=true nodemon --inspect --max-old-space-size=2048 server/",
    "dev:bree": "docker-compose -f docker-compose-dev.yml up -d --remove-orphans && node --inspect --max-old-space-size=2048 node_modules/nuxt/bin/nuxt.js",
    "dev:bree:darwin-arm64": "docker-compose -f docker-compose-dev-arm64.yml up -d --remove-orphans && node --inspect --max-old-space-size=2048 node_modules/nuxt/bin/nuxt.js",
    "dev:darwin-arm64": "docker-compose -f docker-compose-dev-arm64.yml up -d --remove-orphans && NO_BREE=true node --inspect --max-old-space-size=2048 node_modules/nuxt/bin/nuxt.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue,.mjs --ignore-path .gitignore .",
    "start": "node --max-old-space-size=2048 node_modules/nuxt/bin/nuxt.js start",
    "test": "docker-compose -f docker-compose-dev.yml up -d --remove-orphans && NODE_ENV=test mocha --exit test/calendar.test.js test/urls.test.js",
    "test:darwin-arm64": "docker-compose -f docker-compose-dev-arm64.yml up -d --remove-orphans && NODE_ENV=test mocha --exit test/calendar.test.js test/urls.test.js"
  },
  "dependencies": {
    "@dansmaculotte/nuxt-security": "0.0.6",
    "@ladjs/graceful": "3.2.2",
    "@mdi/js": "6.9.96",
    "@nuxtjs/axios": "5.13.6",
    "@nuxtjs/component-cache": "1.1.6",
    "@nuxtjs/google-fonts": "3.1.3",
    "@nuxtjs/moment": "1.6.1",
    "@nuxtjs/pwa": "3.3.5",
    "@nuxtjs/vuetify": "1.12.3",
    "async-wrapper-express-ts": "3.1.6",
    "axios": "0.27.2",
    "bip39": "3.1.0",
    "body-parser": "1.20.2",
    "bree": "9.2.2",
    "cal-parser": "1.0.2",
    "config": "3.3.10",
    "connect-mongodb-session": "3.1.1",
    "cookie-parser": "1.4.6",
    "cookie-universal-nuxt": "2.2.2",
    "core-js": "3.35.0",
    "cors": "2.8.5",
    "dayjs": "1.11.10",
    "encoding": "0.1.13",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "humanize-duration": "3.31.0",
    "jsdom": "23.2.0",
    "luxon": "3.4.4",
    "minify-css-string": "1.0.0",
    "moment": "^2.30.1",
    "mongoose": "8.1.0",
    "ms": "2.1.3",
    "nuxt": "2.17.3",
    "nuxt-start": "2.17.3",
    "route-cache": "0.7.0",
    "sanitize-html": "2.11.0",
    "signale": "1.4.0",
    "undici": "6.4.0",
    "v-tooltip": "2.1.3",
    "vue-plausible": "1.3.2",
    "vue-swatches": "2.1.1",
    "vuetify": "2.7.1",
    "xml2js": "0.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.23.7",
    "@babel/eslint-parser": "7.23.3",
    "@fullhuman/postcss-purgecss": "5.0.0",
    "@nuxt/types": "2.17.3",
    "chai": "4.4.1",
    "css-byebye": "4.0.1",
    "eslint": "8.56.0",
    "eslint-config-standard": "17.1.0",
    "eslint-config-vuetify": "0.6.1",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
