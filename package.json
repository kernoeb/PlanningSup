    "v-tooltip": "2.1.3",
    "vue-plausible": "1.3.2",
    "vue-swatches": "2.1.1",
    "vuetify": "2.7.1",
    "xml2js": "0.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.23.0",
    "@babel/eslint-parser": "7.22.15",
    "@fullhuman/postcss-purgecss": "3.1.3",
    "@nuxt/postcss8": "1.1.3",
    "@nuxt/types": "2.15.8",
    "@nuxtjs/eslint-config": "8.0.0",
    "@nuxtjs/eslint-module": "3.1.0",
    "@nuxtjs/google-fonts": "2.0.0",
    "@nuxtjs/moment": "1.6.1",
    "ajv": "8.12.0",
    "ajv-formats": "2.1.1",
    "chai": "4.3.10",
    "css-byebye": "3.1.1",
