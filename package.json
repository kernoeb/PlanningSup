    "eslint-plugin-standard": "4.1.0",
    "eslint-plugin-vue": "9.20.1",
    "mocha": "10.2.0",
    "nodemon": "3.0.3",
    "sass-loader": "10",
    "supertest": "6.3.4"
  },
  "engines": {
    "node": "20"
  }
}
