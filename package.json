    },
    "catalogs": {
      "types": {
        "@types/bun": "^1.2.21"
      },
      "dev": {
        "vite": "npm:rolldown-vite@latest",
        "@vitejs/plugin-vue": "^6.0.1",
        "typescript": "^5.9.2"
      },
      "ui": {
        "sass-embedded": "^1.92.1",
        "vue": "3.5.21",
        "daisyui": "5.1.9",
        "@tailwindcss/vite": "^4.1.13",
        "tailwindcss": "^4.1.13"
      }
    }
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "bun scripts/run dev",
    "build": "bun scripts/run build",
    "test": "bun scripts/run test",
    "test:unit": "bun scripts/run test:unit",
    "coverage": "bun scripts/run coverage",
    "test:e2e": "bun scripts/run test:e2e",
    "lint": "bun scripts/run lint",
    "lint-fix": "bun scripts/run lint-fix",
    "typecheck": "bun scripts/run typecheck"
