{
  "name": "planningsup",
  "version": "2.19.10",
  "private": "true",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kernoeb/planningsup.git"
  },
  "keywords": [
    "planningsup",
    "planning",
    "university"
  ],
  "workspaces": {
