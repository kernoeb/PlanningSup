{
  "name": "planningsup",
  "version": "2.18.6",
  "private": true,
  "engines": {
    "node": "22"
  },
  "scripts": {
    "build": "node --max-old-space-size=2048 node_modules/nuxt/bin/nuxt.js build",
    "dev": "cross-env NO_BREE=true node --inspect --max-old-space-size=2048 node_modules/nuxt/bin/nuxt.js",
    "dev-server": "cross-env NO_BREE=true node --watch --inspect --max-old-space-size=2048 server/",
    "dev-server:bree": "node --watch --inspect --max-old-space-size=2048 server/",
    "dev-test": "docker compose -f docker-compose-test.yml up -d --build --remove-orphans",
    "dev:bree": "node --inspect --max-old-space-size=2048 node_modules/nuxt/bin/nuxt.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue,.mjs --ignore-path .gitignore .",
    "start": "node --max-old-space-size=2048 node_modules/nuxt/bin/nuxt.js start"
  },
  "dependencies": {
    "@dansmaculotte/nuxt-security": "0.0.6",
    "@ladjs/graceful": "3.2.2",
    "@mdi/js": "6.9.96",
    "@nuxtjs/axios": "5.13.6",
    "@nuxtjs/component-cache": "1.1.6",
    "@nuxtjs/pwa": "3.3.5",
    "async-wrapper-express-ts": "3.1.6",
    "axios": "1.8.3",
    "axios-retry": "4.5.0",
    "body-parser": "1.20.3",
    "bree": "9.2.4",
    "config": "3.3.12",
    "connect-mongodb-session": "5.0.0",
    "cookie-parser": "1.4.7",
    "cookie-universal-nuxt": "2.2.2",
    "core-js": "3.41.0",
    "cors": "2.8.5",
    "express": "4.21.2",
    "express-session": "1.18.1",
    "humanize-duration": "3.32.1",
    "ical.js": "2.1.0",
    "isomorphic-dompurify": "2.22.0",
    "jsdom": "24.1.3",
    "luxon": "3.5.0",
    "minify-css-string": "1.0.0",
    "moment": "2.30.1",
    "mongoose": "8.12.1",
    "ms": "2.1.3",
    "nuxt-start": "2.18.1",
    "route-cache": "0.7.0",
    "signale": "1.4.0",
    "undici": "6.21.2",
    "vue-plausible": "1.3.2",
    "vue-swatches": "2.1.1",
    "xml2js": "0.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.26.10",
    "@babel/eslint-parser": "7.26.10",
    "@fullhuman/postcss-purgecss": "5.0.0",
    "@nuxt/types": "2.18.1",
    "@nuxtjs/google-fonts": "3.2.0",
    "@nuxtjs/moment": "1.6.1",
    "@nuxtjs/vuetify": "1.12.3",
    "@types/connect-mongodb-session": "2.4.7",
    "@types/dompurify": "3.2.0",
    "@types/signale": "1.4.7",
    "@types/supertest": "6.0.2",
    "cross-env": "^7.0.3",
    "css-byebye": "4.0.1",
    "eslint": "8.57.1",
    "eslint-config-standard": "17.1.0",
    "eslint-config-vuetify": "0.6.1",
    "eslint-plugin-html": "8.1.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-promise": "6.6.0",
    "eslint-plugin-standard": "4.1.0",
    "eslint-plugin-vue": "9.33.0",
    "nuxt": "2.18.1",
    "sass-loader": "10",
    "v-tooltip": "2.1.3",
    "vuetify": "2.7.2"
  },
  "overrides": {
    "@nuxtjs/axios": {
      "axios": "$axios",
      "axios-retry": "$axios-retry"
    },
    "eslint-config-vuetify": {
      "@release-it/conventional-changelog": {
        "release-it": {
          "shelljs": "0.8.5"
        }
      }
    }
  }
}
